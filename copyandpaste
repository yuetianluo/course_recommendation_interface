                <br>
              <% } %>
            <% }); %>
            <hr>
            <h5>Rejected Modules</h5>
            <% count = 0 %>
            <% _.each(analytics, function(analytic) { %>
              <% if (analytic.rejected && count < maxCount) { %>
                <%= analytic.name %>: <font color="red" class="pull-right">REJECTED</font>
                <% count += 1 %>
                <br>
              <% } %>
            <% }); %>
            </div>
          </div>
        </div>

